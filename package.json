{
  "name": "reactTest",
  "version": "1.0.0",
  "description": "This is a react starter kit to avoid using create react app",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config utils/build/webpack.config.js --env.env=dev",
    "build": "webpack --config utils/build/webpack.config.js --env.env=prod",
    "build:analyze": "npm run build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "format": "prettier \"src/**/*.{js,jsx,css,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx,css,html}\" --quiet",
    "test": "jest"
  },
  "keywords": [],
  "author": "John Alexander Ferguson",
  "license": "ISC",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.8.6",
    "@testing-library/react": "^8.0.4",
    "@testing-library/react-hooks": "^1.0.4",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.0.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.8.0",
    "jest-axe": "^3.1.1",
    "jest-dom": "^3.5.0",
    "prettier": "^1.18.2",
    "react-axe": "^3.1.0",
    "react-hot-loader": "^4.12.5",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.12",
